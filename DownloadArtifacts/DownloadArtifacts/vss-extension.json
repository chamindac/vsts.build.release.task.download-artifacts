{
  "manifestVersion": 1,
  "id": "chamindac-vsts-release-task-download-artifacts",
  "version": "2.0.2",
  "name": "Download Artifacts",
  "scopes": [ "vso.work", "vso.work_write" ],
  "description": "Download build artifacts with filtering by artifact name, when you have more than one artifact, or download sub folders or files, without downloading the entire artifact folder.",
  "baseUri": "https://localhost",
  "publisher": "chamindac",
  "public": true,
  "icons": {
    "default": "img/logo.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "tags": [
    "build",
    "release",
    "artifacts"
  ],
  "screenshots": [
    {
      "path": "img/screenshots/DownloadArtifacts-AddTask.png"
    },
    {
      "path": "img/screenshots/DownloadArtifacts-01.png"
    },
    {
      "path": "img/screenshots/DownloadArtifacts-02.png"
    }
  ],
  "content": {
    "details": {
      "path": "Docs/readme.md"
    },
    "license": {
      "path": "Docs/license.txt"
    }
  },
  "links": {
    "home": {
      "uri": "https://github.com/chamindac/vsts.release.task.download-artifacts"
    },
    "getstarted": {
      "uri": "https://github.com/chamindac/vsts.release.task.download-artifacts/wiki"
    },
    "learn": {
      "uri": "https://github.com/chamindac/vsts.release.task.download-artifacts/wiki"
    },
    "support": {
      "uri": "https://github.com/chamindac/vsts.release.task.download-artifacts/wiki"
    },
    "repository": {
      "uri": "https://github.com/chamindac/vsts.release.task.download-artifacts"
    },
    "issues": {
      "uri": "https://github.com/chamindac/vsts.release.task.download-artifacts/issues"
    }
  },
  "branding": {
    "color": "rgb(220, 235, 252)",
    "theme": "light"
  },
  "files": [
    {
      "path": "download-artifacts"
    },
    {
      "path": "download-artifacts-new"
    }
  ],
  "categories": [
    "Build and release"
  ],
  "contributions": [
    {
      "id": "download-artifacts",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "type": "ms.vss-distributed-task.task",
      "properties": {
        "name": "download-artifacts"
      }
    },
    {
      "id": "download-artifacts-new",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "type": "ms.vss-distributed-task.task",
      "properties": {
        "name": "download-artifacts-new"
      }
    }
  ]
}
